{
  "expo": {
    "name": "StopWatch4",
    "description": "Multi Timers with get start-times",
    "slug": "StopWatch4",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/stopwatchAppLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/stopwatchAppLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.omrikellner.StopWatch4"
    },
    "android": {
      "versionCode": 21,
      "adaptiveIcon": {
        "foregroundImage": "./assets/stopwatchAppLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.omrikellner.StopWatch4"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "buildToolsVersion": "35.0.0",
            "ndkVersion": "27.0.12077973",
            "useLegacyPackaging": false,
            "enableProguardInReleaseBuilds": false,
            "newArchEnabled": true,
            "extraProguardRules": "-keep class ** { *; }"
          }
        }
      ],
      "expo-sqlite"
    ],
    "web": {
      "favicon": "./assets/stopwatchAppLogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "662634cc-6859-4494-9636-5fc68b63d536"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/662634cc-6859-4494-9636-5fc68b63d536"
    }
  }
}
